<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>one.mwc.mac-audio-default</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/mac-audio-default</string>
        <string>set-rate</string>
        <string>48000</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <false/>
    
    <key>LaunchOnlyOnce</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/var/log/mac-audio-default.log</string>
    
    <key>StandardErrorPath</key>
    <string>/var/log/mac-audio-default-error.log</string>
    
    <key>UserName</key>
    <string>root</string>
    
    <key>ProcessType</key>
    <string>Background</string>
    
    <key>ThrottleInterval</key>
    <integer>60</integer>
    
    <key>WatchPaths</key>
    <array>
        <string>/System/Library/Extensions</string>
        <string>/Library/Audio/Plug-Ins</string>
    </array>
    
    <key>StartCalendarInterval</key>
    <dict>
        <key>Minute</key>
        <integer>0</integer>
        <key>Hour</key>
        <integer>0</integer>
    </dict>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin</string>
    </dict>
    
    <key>WorkingDirectory</key>
    <string>/usr/local/bin</string>
</dict>
</plist>